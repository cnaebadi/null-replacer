{
    "name": "cnaebadi/null-replacer",
    "description": "A Laravel validation rule to remove or replace null fields before validation.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cnaebadi\\NullReplacer\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cnaebadi\\NullReplacer\\NullReplacerServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Sina Ebadi",
            "email": "cnaebadii@yahoo.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^10.2"
    }
}
